@using Golf4.Models
@*@using system.data;*@
@model AdminModels

@{
    ViewBag.Title = "Index";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div class="memberList">
        <h2> Medlemsregister</h2>
        <hr />
    </div>
    <div> 
        <script type="text/javascript" src="https://cdn.datatables.net/v/dt/jq-2.2.4/dt-1.10.13/datatables.min.js"></script>
        <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/v/dt/dt-1.10.13/datatables.min.css" />

        <table id="members">
            <thead>
                <tr>
                   @*@foreach (DataColums item in Model.)
                   {

                   }*@


                </tr>
            </thead>
        </table>
        <script>
        $(document).ready(function () {
            $.ajax({
                "async": "false",
                "url": '/admin/load',
                "type": "GET",
                "data": "json", success: function (result) {
                    $('#members').DataTable({
                        result,
                        "columns": [
                         { "data": "golfid", "autowidth": true },
                         { "data": "lastname", "autowidth": true },
                         { "data": "firstname", "autowidth": true },
                         { "data": "Membercategory", "autowidth": true }

                        ]


                    })
                }
            });
        })
        </script>
    </div>
</body>
</html>
